{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-http": "*",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "itsgoingd/clockwork": "^5.1",
        "jordanbrauer/unit-converter": "dev-master",
        "laravel-validation-rules/credit-card": "^1.6",
        "laravel/cashier": "^13.17",
        "laravel/framework": "^8.12",
        "laravel/sail": "^1.0.1",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "pusher/pusher-php-server": "~4.0",
        "rafaelfragoso/haversini-formula": "^2.0",
        "rap2hpoutre/fast-excel": "^3.0",
        "shuchkin/simplexlsx": "^0.8.26",
        "socialiteproviders/facebook": "^4.1",
        "spatie/laravel-permission": "^5.4",
        "twilio/sdk": "^6.28",
        "tymon/jwt-auth": "^1.0",
        "web64/laravel-cli-colors": "^0.20.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": ["app/globalUtils.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
